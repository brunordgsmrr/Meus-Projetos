package application;

public class Program {

	public static void main(String[] args) {
		// Atividade 5
		ClasseTeste resultado = new ClasseTeste();
		resultado.areaCirculo(3);
		resultado.areaTrapezio(25, 20, 10);
		System.out.println(resultado.somaValores(100));

		System.out.println();
		System.out.println("Aluno 1: " + resultado.avaliaAluno(7, 80));
		System.out.println();
		System.out.println("Aluno 2: " + resultado.avaliaAluno(5, 80));
		System.out.println();
		System.out.println("Aluno 3: " + resultado.avaliaAluno(5, 60));
		System.out.println();

	}

}
