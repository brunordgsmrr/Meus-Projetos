public class Calculadora {

	/**
	 * @param args
	 */	
	public static void main(String[] args) {
		Operadores resultado = new Operadores();
		
		System.out.println("Aluno 1: " + resultado.avaliaAluno(7, 80));
		System.out.println();
		System.out.println("Aluno 2: " + resultado.avaliaAluno(5, 80));
		System.out.println();
		System.out.println("Aluno 3: " + resultado.avaliaAluno(5, 60));
		System.out.println();		

	}

}
